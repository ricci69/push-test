<?xml version='1.0' encoding='utf-8'?>
<widget id="de.appplant.localnotification.example" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>NotificationExample</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <gap:plugin name="de.appplant.cordova.plugin.local-notification" />
    <gap:plugin name="cordova-plugin-app-event" />
    <gap:plugin name="cordova-plugin-app-device" />
    <gap:plugin name="cordova-plugin-app-x-coast" />
    <content src="index.html" />
    <access origin="*" />
    <preference name="windows-target-version" value="10.0" />
</widget>
